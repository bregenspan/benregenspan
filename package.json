{
  "name": "benregenspan",
  "version": "1.0.0",
  "description": "Ben Regenspan Personal Internet Homepage",
  "author": "Ben Regenspan",
  "browserslist": "last 2 versions",
  "scripts": {
    "analyze": "webpack --profile --json > stats.json && ./node_modules/.bin/webpack-bundle-analyzer stats.json",
    "build": "rm -rf dist && cp -r static dist && webpack --optimize-minimize",
    "deploy": "grunt deploy",
    "lint": "eslint ./src/js",
    "serve": "webpack-dev-server --open"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.6",
    "debounce": "^1.0.2",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "fast-memoize": "^2.2.8",
    "file-loader": "^0.11.2",
